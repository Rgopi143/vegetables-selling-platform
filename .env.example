# Environment Variables Example
# Copy this file to .env and fill in your actual values

# API Configuration
VITE_API_URL=https://api.veggiemarket.com
VITE_API_KEY=your-api-key-here

# Application Configuration
VITE_APP_NAME=VeggieMarket
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=Online Vegetable Selling Platform

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_DEBUG=true
VITE_ENABLE_NOTIFICATIONS=true

# Authentication Configuration
VITE_AUTH_DOMAIN_RULES=true
VITE_BUYER_DOMAIN=gmail.com
VITE_SELLER_DOMAIN=veggistore.com
VITE_ADMIN_DOMAIN=ranbidge.com

# Third-party Services
VITE_STRIPE_PUBLIC_KEY=pk_test_your_stripe_public_key
VITE_GOOGLE_ANALYTICS_ID=GA-XXXXXXXXX

# Development Configuration
VITE_DEV_MODE=true
VITE_MOCK_DATA=true

# Database Configuration
VITE_DB_URL=your-database-url-here
VITE_DB_NAME=veggiemarket_db

# Email Configuration
VITE_EMAIL_SERVICE=your-email-service
VITE_EMAIL_FROM=noreply@veggiemarket.com

# File Upload Configuration
VITE_UPLOAD_URL=https://api.veggiemarket.com/upload
VITE_MAX_FILE_SIZE=5MB

# Payment Configuration
VITE_CURRENCY=INR
VITE_PAYMENT_GATEWAY=stripe